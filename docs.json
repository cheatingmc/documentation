{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nice Alts Docs",
  "colors": {
    "primary": "#5865F2",
    "light": "#7289DA",
    "dark": "#4752C4"
  },
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "welcome",
              "faq"
            ]
          },
          {
            "group": "API",
            "pages": [
              "api/api-key",
              "api/stock",
              "api/purchase"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/your-invite",
          "icon": "discord"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Shop",
        "href": "https://nicealts.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "/welcome"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view"
    ]
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/nicealts"
    }
  }
}